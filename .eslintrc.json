{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "semi":"error",
      "import/extensions":[
        "error",
        {"js": "always"}
  
      ],
      "quotes": [
        "error",
        "single",
        { "avoidEscape": true, "allowTemplateLiterals": false }
      ]
      
    }
  }